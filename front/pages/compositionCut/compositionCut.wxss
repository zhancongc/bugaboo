
.container {
  width: 96%;
  margin: 0 auto;
  padding: 200rpx 0;
}
.container-content image {
  vertical-align: center;
}
.angle90 {
  transform:rotate(90deg);
}
.angle180 {
  transform:rotate(180deg);
}
.angle270{
  transform:rotate(270deg);
}
.bottomBar {
  width: 100%;
  height: 80rpx;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  display: flex;
  flex-wrap: wrap;
  border: 1rpx solid #cccccc;
  border-bottom:none;
  border-left:none;
  border-right:none;
  background-color: #ffffff;
}
.bottomBar button {
  width: 50%;
  height: 80rpx;
}
.container-title button{
  position: fixed;
  left: 0rpx;
  bottom: 80rpx;
}